<?php
/**
 * @file dkan_sitewide_demo_front.install
 * Contains install and update functions for Demo Front Page.
 */

/**
 * Implements hook_install().
 */
function dkan_sitewide_demo_front_install() {
  db_update('system')->fields(array('weight' => 99))->condition('name', 'dkan_sitewide_demo_front')->execute();
  drupal_set_message(t("The Demo Front Page has been installed."));
}

