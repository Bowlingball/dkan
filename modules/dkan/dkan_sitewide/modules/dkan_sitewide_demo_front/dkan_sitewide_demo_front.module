/**
 * Implement hook_ctools_plugin_api().
 */
function dkan_sitewide_demo_front_ctools_plugin_api($module, $api) {
  if (($module == 'page_manager' && $api == 'pages_default')
    || ($module == 'panels_mini' && $api == 'panels_default')) {
    return array('version' => 1);
  }
}
